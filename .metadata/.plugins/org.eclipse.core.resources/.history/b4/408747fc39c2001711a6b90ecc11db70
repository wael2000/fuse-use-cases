log4j.logger.camelSpringTest=ERROR, R1

log4j.appender.R1=org.apache.log4j.RollingFileAppender
log4j.appender.R1.File=/Users/redhat/Documents/fuse/out/error/errors.log
log4j.appender.R1.DatePattern=.yyyy-MM-dd.HH
log4j.appender.R1.MaxFileSize=100KB
log4j.appender.R1.MaxBackupIndex=7
log4j.appender.R1.layout=org.apache.log4j.PatternLayout
log4j.appender.R1.layout.ConversionPattern=%p %t %c - %m%n

